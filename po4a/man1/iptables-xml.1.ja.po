"        E<lt>/matchE<gt>\n"
"        E<lt>tcpE<gt>\n"
"          E<lt>sportE<gt>8443E<lt>/sportE<gt>\n"
"        E<lt>/tcpE<gt>\n"
"       E<lt>/conditionsE<gt>\n"
"       E<lt>actionsE<gt>\n"
