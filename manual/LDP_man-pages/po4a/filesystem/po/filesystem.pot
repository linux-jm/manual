#. type: Plain text
#: build/C/man2/ustat.2:33
msgid "ustat - get file system statistics"
msgstr ""

#. type: Plain text
#: build/C/man2/ustat.2:36
#, no-wrap
msgid "B<#include E<lt>sys/types.hE<gt>>\n"
msgstr ""

#. type: Plain text
#: build/C/man2/ustat.2:38
#, no-wrap
msgid "B<#include E<lt>unistd.hE<gt>>    /* libc[45] */\n"
msgstr ""

