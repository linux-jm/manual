#~ msgid ""
#~ "LIST_HEAD(listhead, entry) head;\n"
#~ "struct listhead *headp;                 /* List head. */\n"
#~ "struct entry {\n"
#~ "    ...\n"
#~ "    LIST_ENTRY(entry) entries;          /* List. */\n"
