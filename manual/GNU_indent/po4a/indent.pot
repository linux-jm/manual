msgstr ""

#. type: TP
#: original/man1/indent.1:421
#, no-wrap
msgid "B<-sbi>I<n>, B<--struct-brace-indentation>I<n>"
msgstr ""

#. type: Plain text
#: original/man1/indent.1:424
msgid "Indent braces of a struct, union or enum N spaces."
msgstr ""

#. type: TP
#: original/man1/indent.1:426
#, no-wrap
msgid "B<-sc>, B<--start-left-side-of-comments>"
msgstr ""

#. type: Plain text
#: original/man1/indent.1:429
