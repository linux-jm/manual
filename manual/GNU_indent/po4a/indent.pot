#, no-wrap
msgid "B<-T>"
msgstr ""

#. type: Plain text
#: original/man1/indent.1:450
msgid "Tell B<indent> the name of typenames."
msgstr ""

#. type: TP
#: original/man1/indent.1:452
#, no-wrap
msgid "B<-ts>I<n>, B<--tab-size>I<n>"
msgstr ""

#. type: Plain text
