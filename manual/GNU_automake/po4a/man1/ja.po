#: original/man1/automake-1.16.1:103
#, no-wrap
msgid "ABOUT-NLS"
msgstr "ABOUT-NLS"

#. type: Plain text
#: original/man1/automake-1.16.1:106
msgid "ar-lib libversion.in missing"
