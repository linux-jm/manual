#: original/man1/lpoptions.1:129
msgid "I<cancel(1)>, I<lp(1)>, I<lpadmin(8)>, I<lpr(1)>,"
msgstr "I<cancel(1)>, I<lp(1)>, I<lpadmin(8)>, I<lpr(1)>,"

