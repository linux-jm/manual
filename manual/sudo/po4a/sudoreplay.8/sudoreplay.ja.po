#. type: Plain text
#: sudoreplay.man:405
#, no-wrap
msgid "# sudoreplay -l user jeff command '/bin/[a-z]*sh'\n"
msgstr "# sudoreplay -l user jeff command '/bin/[a-z]*sh'\n"

#. type: Plain text
#: sudoreplay.man:409
msgid "List sessions run by jeff or bob on the console:"
